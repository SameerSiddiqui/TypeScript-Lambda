{
  "name": "TypeScript Lambda",
  "version": "0.0.0",
  "description": "Sample API Service Implementation on Lambda",
  "author": "Sameer Siddiqui <Sid7201@gmail.com>",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "git@github.com:SameerSiddiqui/TypeScript-Lambda.git"
  },
  "license": "MIT",
  "keywords": [
    "lambda"
  ],
  "scripts": {
    "setup-local": "npm install && node-prune",
    "format": "prettier . --write",
    "lint": "eslint .",
    "lint:fix": "eslint --ext .js,ts --fix \"./src/**/*\"",
    "build": "rm -rf dist && tsc",
    "build:win": "if exist dist rd /s /q dist && tsc || mkdir dist && tsc",
    "start:dev": "nodemon --watch ./src -e ts --exec 'npm run build && sam local invoke' ",
    "start:dev:win": "nodemon --watch ./src -e ts --exec \"npm run build:win && sam local invoke\"",
    "test": "jest --runInBand",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "deploy": "sam build && sam deploy"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "devDependencies": {
    "@middy/core": "^6.0.0",
    "@middy/http-error-handler": "4.7.0",
    "@middy/http-header-normalizer": "4.7.0",
    "@middy/http-json-body-parser": "4.7.0",
    "@middy/http-router": "4.7.0",
    "@middy/http-security-headers": "4.7.0",
    "@middy/input-output-logger": "4.7.0",
    "@middy/ssm": "^6.0.0",
    "@middy/util": "^6.0.0",
    "tsyringe": "^4.7.0",
    "jsonwebtoken": "^8.5.1",
    "jwks-rsa": "^1.7.0",
    "reflect-metadata": "^0.1.13",
    "uuid": "^9.0.1",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^5.44.0",
    "@typescript-eslint/parser": "^5.44.0",
    "dotenv": "^16.3.1",
    "eslint": "^8.49.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.1.4",
    "eslint-plugin-jest-formatting": "^3.1.0",
    "eslint-plugin-sonarjs": "^0.16.0",
    "jest": "^29.7.0",
    "js-yaml": "^4.1.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^4.9.5",
    "@types/aws-lambda": "^8.10.131",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/node": "^18.19.4"
  }
}
